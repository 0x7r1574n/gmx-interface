{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "utils/*": ["utils/*"],
      "modules/*": ["modules/*"],
      "configs/*": ["configs/*"],
      "types/*": ["types/*"],
      "abis/*": ["abis/*"],
      "swap/*": ["swap/*"],
      "prebuilt": ["prebuild/index"]
    },
    "plugins": [
      { "transform": "typescript-transform-paths" },
      { "transform": "typescript-transform-paths", "afterDeclarations": true }
    ]
  }
}
