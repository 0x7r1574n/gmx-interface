{
  "name": "@gmx-io/sdk",
  "version": "0.2.0",
  "type": "module",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/types/index.d.ts"
    },
    "./abis/*": {
      "import": "./build/esm/abis/*",
      "require": "./build/cjs/abis/*",
      "types": "./build/types/abis/*"
    },
    "./prebuilt/*": {
      "import": "./build/esm/prebuilt/*",
      "require": "./build/cjs/prebuilt/*",
      "types": "./build/types/prebuilt/*"
    },
    "./utils/*": {
      "import": "./build/esm/utils/*",
      "require": "./build/cjs/utils/*",
      "types": "./build/types/utils/*"
    },
    "./types/*": {
      "import": "./build/esm/types/*",
      "require": "./build/cjs/types/*",
      "types": "./build/types/types/*"
    },
    "./configs/*": {
      "import": "./build/esm/configs/*",
      "require": "./build/cjs/configs/*",
      "types": "./build/types/configs/*"
    }
  },
  "scripts": {
    "prebuild": "yarn tsx scripts/prebuild",
    "prepare": "ts-patch install -s",
    "copy:jsons": "cp -r src/abis build/esm && cp -r src/prebuilt build/esm && cp -r src/abis build/cjs && cp -r src/prebuilt build/cjs",
    "test": "vitest",
    "test:ci": "vitest run",
    "tscheck": "tsc -p tsconfig.json --noEmit",
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build": "rm -rf build && yarn compile && yarn copy:jsons"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cross-fetch": "4.0.0",
    "graphql": "15.8.0",
    "isomorphic-performance": "5.1.1",
    "lodash": "4.17.21",
    "query-string": "7.1.1",
    "typescript": "5.4.2",
    "universal-perf-hooks": "1.0.1",
    "viem": "^2.7.20"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/lodash": "4.14.198",
    "@types/node": "18.7.13",
    "lint-staged": "12.3.4",
    "ts-patch": "3.2.1",
    "tsx": "4.19.0",
    "typescript-transform-paths": "3.5.1",
    "vitest": "3.0.4"
  },
  "files": [
    "build"
  ],
  "packageManager": "yarn@3.1.0"
}
